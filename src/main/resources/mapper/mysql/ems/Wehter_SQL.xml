<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Wether"> 


	<insert id="DateInsert">
		INSERT INTO WETHER_TABLE
		(
			NOWDATE,RAINRATEAM,TEMPERATUREAM,SKYAM,RAINRATEPM,TEMPERATUREPM,SKYPM,TEMPERATUREMIN,TEMPERATUREMAX,CLOUDRATE
		)
		VALUES
		<foreach separator="," item="item" collection="list">
		(
			#{item.NowDate},#{item.rainRateAm},#{item.temperatureAm},#{item.skyAm},#{item.rainRatePm},#{item.temperaturePm},#{item.skyPm},#{item.temperatureMin},#{item.temperatureMax},#{item.CLOUDRATE}
		) 
		</foreach>
	</insert>
	
	<delete id="DeleteDate">
		DELETE FROM WETHER_TABLE
	</delete>
	
	<select id="SelectDate" resultType="HashMap">
		SELECT * FROM WETHER_TABLE 
	</select>
	
	<select id="TodayDate" resultType="HashMap">
		SELECT * FROM WETHER_TABLE LIMIT 1
	</select>
</mapper>